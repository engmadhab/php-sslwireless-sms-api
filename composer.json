{
    "name": "madhab/php-sslwireless-sms",
    "description": "PHP client for SSL Wireless SMS API",
    "keywords": ["SSL Wireless", "php", "api", "sms", "php-ssl-sms"],
    "type": "library",
    "require": {
        "php": ">=5.5.9",
        "guzzlehttp/guzzle": "^6.2",
        "ext-simplexml": "*"
    },

    "license": "MIT",
    "authors": [
        {
            "name": "madhab Bin Azhar",
            "email": "madhab.dns@gmail.com"
        }
    ],

    "autoload": {
        "psr-4": {
